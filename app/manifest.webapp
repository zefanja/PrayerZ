        "32": "/biblez-ng/app/assets/biblez32.png",
        "60": "/biblez-ng/app/assets/biblez60.png",
        "90": "/biblez-ng/app/assets/biblez90.png",
        "128": "/biblez-ng/app/assets/biblez128.png",
        "256": "/biblez-ng/app/assets/biblez256.png",
        "512": "/biblez-ng/app/assets/biblez512.png"
    },
